class Solution(object):
    def permuteUnique(self, nums):
        res = []
        nums.sort(
            used = [false] * len(nums)

            def bacltrack(path):
                if len(path) == len(nums):
                    res.append(path[:])
                    return

                for i in range(len(nums)):
                    if used[i]:
                        continue

                    if i > 0 and nums[i] == nums[i - 1] and not used[i - 1]:]:
                        continue

                    used[i] = true
                    path.append(nums[i])
                    backtrack(path)
                    path.pop()
                    used[i] = false

            backtrack([])
            return res   
